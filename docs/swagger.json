{
    "swagger": "2.0",
    "info": {
        "description": "Test task for Effective Mobile",
        "title": "Task tracker api",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/",
    "paths": {
        "/jobs": {
            "post": {
                "description": "Assign user to a task by user id and task id first post starts the task and second stops it",
                "consumes": [
                    "application/json"
                ],
                "summary": "Assign user to a task",
                "parameters": [
                    {
                        "description": "{",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.AddJobDto"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "status OK"
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/helpers.ErrorDto"
                        }
                    },
                    "422": {
                        "description": "Unprocessable Entity",
                        "schema": {
                            "$ref": "#/definitions/helpers.ErrorDto"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/helpers.ErrorDto"
                        }
                    }
                }
            }
        },
        "/jobs/user": {
            "get": {
                "description": "return list of jobs for user by user id",
                "produces": [
                    "application/json"
                ],
                "summary": "Get list of jobs for user by user id",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "User ID",
                        "name": "id",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "description": "page",
                        "name": "page",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "description": "limit",
                        "name": "limit",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "returning Users objects",
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/definitions/models.ShowUserDto"
                                },
                                {
                                    "type": "object",
                                    "properties": {
                                        "TotalWorkTime": {
                                            "type": "object"
                                        }
                                    }
                                }
                            ]
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/helpers.ErrorDto"
                        }
                    }
                }
            }
        },
        "/tasks": {
            "post": {
                "description": "Creates new task",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "Creates new task",
                "parameters": [
                    {
                        "description": "new task data",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.CreateTaskDto"
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "returning task object",
                        "schema": {
                            "$ref": "#/definitions/models.Task"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/helpers.ErrorDto"
                        }
                    },
                    "422": {
                        "description": "Unprocessable Entity",
                        "schema": {
                            "$ref": "#/definitions/helpers.ErrorDto"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/helpers.ErrorDto"
                        }
                    }
                }
            }
        },
        "/tasks/{id}": {
            "delete": {
                "description": "Deletes task by id",
                "summary": "Delete task",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "Task ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "{Deleted id: id}",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "422": {
                        "description": "Unprocessable Entity",
                        "schema": {
                            "$ref": "#/definitions/helpers.ErrorDto"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/helpers.ErrorDto"
                        }
                    }
                }
            }
        },
        "/users": {
            "get": {
                "description": "return list of users",
                "produces": [
                    "application/json"
                ],
                "summary": "Get list of users",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "User ID",
                        "name": "id",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "User name",
                        "name": "name",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "description": "User surname",
                        "name": "surname",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "User patronymic",
                        "name": "patronymic",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "User address",
                        "name": "address",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "User passpor tnumber",
                        "name": "passportnumber",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "description": "page",
                        "name": "page",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "description": "limit",
                        "name": "limit",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "returning Users objects",
                        "schema": {
                            "type": "array",
                            "items": {
                                "allOf": [
                                    {
                                        "$ref": "#/definitions/models.ShowUserDto"
                                    },
                                    {
                                        "type": "object",
                                        "properties": {
                                            "Jobs": {
                                                "type": "object"
                                            }
                                        }
                                    }
                                ]
                            }
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/helpers.ErrorDto"
                        }
                    }
                }
            },
            "post": {
                "description": "Creates new user by passport number",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "Creates new user",
                "parameters": [
                    {
                        "description": "Passport number in xxxx xxxxxx format",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.PassportNumberDto"
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "returning User object",
                        "schema": {
                            "$ref": "#/definitions/models.User"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/helpers.ErrorDto"
                        }
                    },
                    "422": {
                        "description": "Unprocessable Entity",
                        "schema": {
                            "$ref": "#/definitions/helpers.ErrorDto"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/helpers.ErrorDto"
                        }
                    }
                }
            }
        },
        "/users/{id}": {
            "delete": {
                "description": "Deletes a user by id",
                "summary": "Delete a user",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "User ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "{Deleted id: id}",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "422": {
                        "description": "Unprocessable Entity",
                        "schema": {
                            "$ref": "#/definitions/helpers.ErrorDto"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/helpers.ErrorDto"
                        }
                    }
                }
            },
            "patch": {
                "description": "Updates a user by id",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "Updates a user",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "User ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    },
                    {
                        "description": "fields need to be updated",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.UpdateUserDto"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "returning User object",
                        "schema": {
                            "$ref": "#/definitions/models.User"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/helpers.ErrorDto"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "helpers.ErrorDto": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer"
                },
                "message": {
                    "type": "string"
                }
            }
        },
        "models.AddJobDto": {
            "type": "object",
            "required": [
                "taskid",
                "userid"
            ],
            "properties": {
                "taskid": {
                    "type": "integer",
                    "minimum": 0
                },
                "userid": {
                    "type": "integer",
                    "minimum": 0
                }
            }
        },
        "models.CreateTaskDto": {
            "type": "object",
            "required": [
                "name"
            ],
            "properties": {
                "name": {
                    "type": "string",
                    "minLength": 3
                }
            }
        },
        "models.PassportNumberDto": {
            "type": "object",
            "required": [
                "passportNumber"
            ],
            "properties": {
                "passportNumber": {
                    "type": "string",
                    "example": "1234 123456"
                }
            }
        },
        "models.ShowUserDto": {
            "type": "object",
            "properties": {
                "address": {
                    "type": "string",
                    "example": "г. Екатеринбург, ул. Берии, д. 7, кв. 3"
                },
                "jobs": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/models.ShowUserTaskDto"
                    }
                },
                "name": {
                    "type": "string",
                    "example": "Иван"
                },
                "passport_number": {
                    "type": "string",
                    "example": "1234 123456"
                },
                "patronymic": {
                    "type": "string",
                    "example": "Иванович"
                },
                "surname": {
                    "type": "string",
                    "example": "Иванов"
                },
                "total_work": {
                    "type": "string",
                    "example": "1h 1m"
                },
                "user_id": {
                    "type": "integer",
                    "example": 1
                }
            }
        },
        "models.ShowUserTaskDto": {
            "type": "object",
            "properties": {
                "task_id": {
                    "type": "integer",
                    "example": 1
                },
                "task_name": {
                    "type": "string",
                    "example": "Idle"
                },
                "time_on_task": {
                    "type": "string",
                    "example": "1h 1m"
                }
            }
        },
        "models.Task": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "models.UpdateUserDto": {
            "type": "object",
            "properties": {
                "address": {
                    "type": "string",
                    "minLength": 28,
                    "example": "г. Екатеринбург, ул. Берии, д. 7, кв. 3"
                },
                "name": {
                    "type": "string",
                    "minLength": 5,
                    "example": "Иванов"
                },
                "passportNumber": {
                    "type": "string",
                    "example": "1234 123456"
                },
                "patronymic": {
                    "type": "string",
                    "minLength": 3,
                    "example": "Иванович"
                },
                "surname": {
                    "type": "string",
                    "minLength": 3,
                    "example": "Иванов"
                }
            }
        },
        "models.User": {
            "type": "object",
            "properties": {
                "adress": {
                    "type": "string",
                    "example": "г. Екатеринбург, ул. Берии, д. 7, кв. 3"
                },
                "id": {
                    "type": "integer",
                    "example": 13
                },
                "name": {
                    "type": "string",
                    "example": "Иван"
                },
                "passportNumber": {
                    "type": "string",
                    "example": "1234 123456"
                },
                "patronymic": {
                    "type": "string",
                    "example": "Иванович"
                },
                "surname": {
                    "type": "string",
                    "example": "Иванов"
                }
            }
        }
    }
}